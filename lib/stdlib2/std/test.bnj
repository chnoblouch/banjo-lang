use std.{system.exit, convert.to_repr};

func assert_eq[T](a: T, b: T) {
    if a != b {
        print("assertion failed: ");
        print(to_repr(a));
        print(" != ");
        println(to_repr(b));
        exit(1);
    }
}

func assert_ne[T](a: T, b: T) {
    if a == b {
        print("assertion failed: ");
        print(to_repr(a));
        print(" == ");
        println(to_repr(b));
        exit(1);
    }
}

func fail() {
	print("test failed");
}
