#! output: write,file;write,socket;read,file;read,socket;

proto Stream {
    func write(self);
    func read(self);
}

struct File : Stream {
    func write(self) {
        print("write,file;");
    }

    func read(self) {
        print("read,file;");
    }
}

struct Socket : Stream {
    func write(self) {
        print("write,socket;");
    }

    func read(self) {
        print("read,socket;");
    }
}

func write_to(stream: *Stream) {
    stream.write();
}

func read_from(stream: *Stream) {
    stream.read();
}

func main() {
    var file = File {};
    var socket = Socket {};

    write_to(&file);
    write_to(&socket);
    read_from(&file);
    read_from(&socket);
}