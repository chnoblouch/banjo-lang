# test:subtest
# test:output "054"

func add(a: i32, b: i32) -> i32 {
	return a + b;
}

func inc(a: i32) -> i32 {
	return a + 1;
}

func main() {
	print(0);
	print(add(2, 3));
	print(inc(add(1, 2)));
}

