func testy(a: i32, b: i32, c: i32, d: i32, e: i32, f: i32) {
	print(a);
	print(b);
	print(c);
	print(d);
	print(e);
	print(f);
}

func main() {
	print("a");
	testy(1, 2, 3, 4, 5, 6);
	print("b");
}

