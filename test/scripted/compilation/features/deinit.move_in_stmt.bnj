# test:output "abrcd"

func consume(resource: Resource) -> bool {
    print('b');
    return true;
}

func main() {
    var resource = Resource {};

    print('a');

    if false {
        print('!');
    } else if false {
        print('!');
    } else if consume(resource) {
        print('c');
