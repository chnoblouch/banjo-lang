# test:common

struct Resource {
    func __deinit__(self) {
        print('r');
    }
}

struct ValueResource {
    var value: i32;
    
    func __deinit__(self) {
        print('v');
        print(self.value);
    }
}

# test:subtest
# test:output "abrcd"

func consume(resource: Resource) -> bool {
    print('b');
    return true;
}

func main() {
    var resource = Resource {};

    print('a');

    if consume(resource) {
        print('c');
    }

    print('d');
}

