# test:output "95,12,35,86,48:Table,27:Lamp,"

struct Object {
	var id: i32;
	var name: *u8;
}

func main() {
	var ints = [95, 12, 35, 86];
	for *int in ints {
		print(*int);
		print(',');
	}
	
	var objects = [
		Object { id: 48, name: "Table" },
		Object { id: 27, name: "Lamp" },
	];
	
	for *object in objects {
		print((*object).id);
		print(':');
		print((*object).name);
		print(',');
	}
}
