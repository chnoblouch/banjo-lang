# test:subtest
# test:output "5,8,12,17,;2,4,6,8,"

func main() {
	var array = [5, 8, 12, 17];
	
	for value in array.copy() {
		print(value);
		print(',');
	}
	print(';');
	
	for *value in array {
		*value = *value / 2;
	}
	
	for value in array {
		print(value);
		print(',');
	}
}

# test:subtest
# test:output "95,12,35,86,48:Table,27:Lamp,"

struct Object {
	var id: i32;
	var name: *u8;
}

func main() {
	var ints = [95, 12, 35, 86];
	for int in ints {
		print(int);
		print(',');
	}
	
	var objects = [
		Object { id: 48, name: "Table" },
		Object { id: 27, name: "Lamp" },
	];
	
	for object in objects {
		print(object.id);
		print(':');
		print(object.name);
		print(',');
	}
}

