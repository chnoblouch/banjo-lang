#! output: 7 12

struct Point {
    var x: i32;
    var y: i32;

	func set(self, x: i32, y: i32) {
		self.x = x;
		self.y = y;
	}

    func println(self) {
        print(self.x);
        print(" ");
		print(self.y);
    }
}

func main() {
    var point = Point { x: 0, y: 0 };
	point.set(7, 12);
    point.println();
}