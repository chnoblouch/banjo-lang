# test:subtest
# test:output "true,3,7"

struct Point {
    var x: i32;
    var y: i32;
}

func main() {
    var point: ?Point = Point {
        x: 3,
        y: 7,
    };
    
    print(point.has_value);
    print(",");
    print(point.value.x);
    print(",");
    print(point.value.y);
}

