# test:subtest
# test:output 5,-2

struct Point {
    var x: i32;
    var y: i32;
    
    func __add__(self, rhs: Point) -> Point {
        return Point {
            x: self.x + rhs.x,
            y: self.y + rhs.y,
        };
    }
}

func main() {
    var a = Point {
        x: 3,
        y: 10,
    };
    
    var b = Point {
        x: 2,
        y: -12,
    };
    
    var c = a + b;
    
    print(c.x);
    print(",");
    print(c.y);
}

